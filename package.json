{
  "name": "browser-ext-storage-x",
  "version": "0.0.9",
  "displayName": "StorageX",
  "description": "StorageX extension is a toolset to manage browser's Application Storage",
  "author": {
    "name": "Rahul Kurup",
    "email": "me@rahulkurup.com"
  },
  "maintainers": [
    "Piyush Gupta (https://github.com/piyu-sh/)",
    "Rahul Kurup <me@rahulkurup.com> (https://rahulkurup.com/)"
  ],
  "private": true,
  "bugs": {
    "email": "me@rahulkurup.com"
  },
  "engines": {
    "node": ">=16.18.0",
    "yarn": "1.22.19"
  },
  "volta": {
    "node": "16.18.0"
  },
  "scripts": {
    "publish:chrome": "node store/publish/chrome.mjs",
    "publish:edge": "node store/publish/edge.mjs",
    "publish": "concurrently npm:publish:chrome npm:publish:edge",
    "clean": "rimraf dist",
    "prepare": "husky install",
    "compress": "node scripts/compress.mjs",
    "prebuild": "yarn clean",
    "build": "cross-env NODE_ENV=production webpack",
    "postbuild": "yarn compress",
    "prebuild:local": "yarn clean",
    "build:local": "cross-env NODE_ENV=development webpack",
    "postbuild:local": "yarn compress",
    "build:chrome": "cross-env NODE_ENV=development webpack --config ./webpack/chrome.js",
    "build:firefox": "cross-env NODE_ENV=development webpack --config ./webpack/firefox.js",
    "prewatch": "yarn clean",
    "watch": "cross-env NODE_ENV=development webpack -w",
    "watch:chrome": "cross-env NODE_ENV=development webpack --config ./webpack/chrome.js -w",
    "watch:firefox": "cross-env NODE_ENV=development webpack --config ./webpack/firefox.js -w",
    "start": "concurrently npm:watch:chrome npm:watch:firefox",
    "pretty": "prettier --loglevel=error --write ."
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,html,css,json,md}": "prettier --loglevel=error --write"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mantine/core": "^5.7.1",
    "@mantine/hooks": "^5.7.1",
    "emotion": "^11.0.0",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.2",
    "@babel/plugin-transform-react-jsx": "^7.19.0",
    "@babel/preset-env": "^7.20.2",
    "@emotion/babel-plugin": "^11.10.5",
    "@plasmohq/edge-addons-api": "^1.1.0",
    "@types/chrome": "^0.0.200",
    "@types/lodash": "^4.14.188",
    "@types/react-dom": "^18.0.8",
    "@types/webextension-polyfill": "^0.9.1",
    "babel-loader": "^9.1.0",
    "chrome-webstore-upload": "^1.0.0",
    "concurrently": "^7.5.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "dotenv": "^16.0.3",
    "dotenv-expand": "^9.0.0",
    "generate-json-from-js-webpack-plugin": "^0.1.1",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.3",
    "mkdirp": "^1.0.4",
    "node-args": "^2.1.8",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.6",
    "ts-loader": "^9.4.1",
    "typescript": "^4.8.4",
    "webextension-polyfill": "^0.10.0",
    "webpack": "^5.74.0",
    "webpack-bundle-analyzer": "^4.7.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0",
    "webpack-shell-plugin-next": "^2.3.1",
    "zip-lib": "^0.7.3"
  }
}
